{
  "name": "bg-splash",
  "version": "1.0.0",
  "description": "Downloads and applies background image from Unsplash",
  "main": "src/index.ts",
  "author": "Dan <dan@danitt.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "yarn fetch:unsplash",
    "fetch:unsplash": "ts-node -r tsconfig-paths/register ./src/index --source unsplash",
    "fetch:inspiro": "ts-node -r tsconfig-paths/register ./src/index --source inspiro",
    "lint": "tslint --project tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@types/minimist": "^1.2.0",
    "dotenv": "8.2.0",
    "minimist": "^1.2.5",
    "node-fetch": "2.6.0",
    "qs": "6.9.7",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/node-fetch": "2.5.5",
    "@types/qs": "6.9.1",
    "jest": "25.1.0",
    "ts-jest": "25.2.1",
    "tslint": "6.1.0",
    "tslint-config-airbnb": "5.11.2"
  }
}
